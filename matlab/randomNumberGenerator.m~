function [random] = randomNumberGenerator(a, c, max, seed, n)
% Generater random test data for D5
% seed = X(0)
% n = length
% a,c,m according to formula of task D5

% Preallocate return array
%random = zeros(n, 1);
% Set seed
random(1) = seed;

for i = 2:n
    random(i) = mod(round((a * random(i-1) + c)), round(max))
    %fprintf('(%d * %d + %d) mod %d',a,random(i-1),c,round(max))
    a = i
end

